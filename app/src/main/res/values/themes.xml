<resources xmlns:tools="http://schemas.android.com/tools">
  <!-- Base application theme. -->
  <style name="Theme.FundingRates" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
    <!-- Primary brand color. -->
    <item name="actionBarStyle">@style/Custom.ActionBar</item>
    <item name="colorPrimary">#2296F3</item>
    <item name="colorPrimaryVariant">#0477D3</item>
    <item name="colorOnPrimary">@color/white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">#FFF</item>
    <item name="colorSecondaryVariant">#717171</item>
    <item name="colorOnSecondary">#4E4E4E</item>
    <item name="colorControlHighlight">#4FB1FF</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    <!-- Customize your theme here. -->
  </style>

  <style name="Custom.ActionBar" parent="Widget.MaterialComponents.Light.ActionBar.Solid">
    <item name="backgroundColor">#0477D3</item>
    <item name="elevation">0dp</item>
  </style>

</resources>